}


()_ thatMothod($_ s, $_ d) {

    READ(s);

    WRITE(s);

    s <- s;

    -> d;

}

#_
()_ thatMothod() {

    x <- 10;
    y <- 20;
    ? (10){
        x <- 10;
        x <- 10;
    }
    -> 100;
   
}
_#

()_ f($_ i){
    $_ x;
    $_ y;
    y <- 100;

    loop(x <- 10; x < 120; x <- x + 1){
        y <- y - 1;
        z <- (z - 1) * (z + 2) / 3;
    }
}