()_ thatMothod($_ s, $_ d) {

    READ(s);

    WRITE(s);

    s <- s;

    -> d;

}

#_
()_ thatMothod() {

    x <- 10;
    y <- 20;
    ? (10){
        x <- 10;
        x <- 10;
    }
    -> 100;
   
}
