()_ f(){

    $_ x;
    loop ( x < 0 ) {
        x <- 1;
        ?( x = 2 ){
            x <- 3;
            ?( x = 4 ){
                break;
            }
            : () {
                x <- 5;
                break;
            }
            
        }
        x <- 4;
    }
    # $_ y;
