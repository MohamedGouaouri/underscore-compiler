#_
()_ g()
{
    #$_ x;
    $_ y;
    y <- 100;
    $_ z <- 0;

    continue; #error

    loop (x < 10){
        y <- y + 1;
        x <- x + 1;
        loop (i <- 0; x > 10; i <- i+1){
            ? (x = 2){
                x <- x+1;
            } 
            : (){
                loop (i <- 1; i < 3; i <- i+1){
                    x <- x + 1;
                    ? (x = 3){
                        continue;
                    }
                }
                
            }
        }
        break;
    }

    break; #error
}
_#

()_ f(){

    $_ x <- 110;
    loop ( x < 200 ) {
        x <- 0;

        ? ( x = 120 ) {
            x <- 1;
            break;
        } 

        loop(x <- 2; x <= 3; x<-x+4) {
            x <- 5;
            ? (true) {
                x <- 6;
                break;
                continue;
                x <- 7;
            }
            :() {
                x <- 8;
                break;
            }
        }
        
        
        x <- 9;
    }

    $_ y <- 10;
}

