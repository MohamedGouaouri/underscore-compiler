
()_ f(){

    $_ x;
    loop ( x = 0 ) {
        x <- 1;
        break;
        loop(false){
            x <- 3;
            ? (true) {
                x <- 4;
                break;
            }
        }
        x <- 5;
    }
    x <- 6;

    loop(x <- 0; x > 0; x <- x*x) {
        continue;
        ?(false) {
            break;
        }
    }
    loop($_x <- 0; x > 0; x <- x*x) {
        continue;
        ?(false) {
            break;
        }
    }


    ? (true) {
        break;
    }

    loop ( ( ( x > 0  || ( x < 10 ) && ( x< 10) ) ) )  {
        x <- 1;
        ?( x = 2 ){
            x <- 3;
            ?( x = 4 ){
                break;
                break;
            }
            : () {
                x <- 5;
                break;
                break;
            }
            x <- 6;
        }
        x <- 7;
    }
    # x <- 1 + (3 + (5 * 4 * (4 * 4 * ( 2 + 1))));
    # $_y;
    # loop(true){
    #     x <- x * 1;
    #     loop(true){
    #         loop()
    #     }
    #     x <- x + 1;
    # }
    # x <- x + 3;
    # $_ y;
    # loop(true){
    #     ? ( ( (true && ( false && false )) ) ){
    #         x <- x * 3;
    #         break;
    #     }
    #     break;
    # }

}

